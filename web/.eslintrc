{
  parser: "babel-eslint",
  "rules": {
    "react/destructuring-assignment": [<enabled>, "never"],
    "no-console": 2,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "no-unused-vars": ["error", { "args": "none" }],
    "react/prefer-stateless-function": 0
  },
  "extends": ["airbnb"],
  "env": {
    "browser": true
  }
}
